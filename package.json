  "devDependencies": {
    "babel-preset-latest": "^6.24.1",
    "cssnano": "^3.10.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-license": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.3",
